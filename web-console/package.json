{
  "name": "react-template",
  "version": "1.0.0",
  "description": "A frontend project template for React.js",
  "author": "tiexin.gong <tiexin.gong@qq.com>",
  "private": true,
  "homepage": "https://github.com/gongtiexin/react-template#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gongtiexin/react-template.git"
  },
  "bugs": {
    "url": "https://github.com/gongtiexin/react-template.git/issues"
  },
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js",
    "build": "rimraf ./dist && webpack --progress --config webpack.config.production.js",
    "loading": "rimraf ./dist/static/html/loading && webpack --config static/html/loading/webpack.loading.config.js",
    "prettier": "prettier -c --write '**/*'"
  },
  "lint-staged": {
    "*": [
      "yarn prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "React",
    "React-Router-v4",
    "MobX",
    "Webpack",
    "ESNEXT"
  ],
  "browserslist": [
    "> 1%",
    "last 1 version",
    "not dead",
    "not ie < 11"
  ],
  "license": "ISC",
